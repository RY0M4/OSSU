; signature: Number -> Number
; purpose: doubles n
; stub: (define (double n) 0)

(check-expect (double 0) 0)
(check-expect (double 4) 8)
(check-expect (double 3.3) (* 2 3.3))
(check-expect (double -1) -2)

; template:
; (define (double n)
;   (* (2 n)))

(define (double n)
  (* 2 n))

(double 16)
